  "author": "fi3ework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inkerjs/inking"
  },
  "bugs": {
    "url": "https://github.com/inkerjs/inking"
  },
  "homepage": "https://github.com/inkerjs/inking",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^11.11.4",
    "coveralls": "^3.0.3",
    "envify": "^4.1.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^23.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rollup": "^1.7.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "tslint-config-alloy": "^0.2.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
